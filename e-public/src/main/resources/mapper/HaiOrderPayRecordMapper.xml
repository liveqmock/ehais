	left join hai_donation d on d.donation_id = o.table_id
	where o.wxid = #{condition.store_id} and o.table_name = #{tableName} 
	<if test="tableId != null " >
      <![CDATA[ and o.table_id = ${tableId}  ]]>
    </if>
    <if test="orderStatus != null " >
      <![CDATA[ and o.order_status = ${orderStatus}  ]]>
    </if>
    order by o.create_date desc
    <if test="start != null and len != null and start >= 0 and len > 0" >
      <![CDATA[ limit #{start,jdbcType=INTEGER},#{len,jdbcType=INTEGER}  ]]>
    </if>
  </select>
  
  
</mapper>